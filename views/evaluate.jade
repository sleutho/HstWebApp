extends layout

append head
  script.
    var study = !{JSON.stringify(study)};
  script(src="/javascripts/evaluate.js")
 
append backward
  p
    a(href="/studies/" + study + "/specification") Back to Specification

append content
  #msg Status
  ul
    li#status
  input(id="eval" type="button", size="30", value="Evaluate")

append forward
  p
    a(href="/studies/" + study + "/summary") Next to Summary

