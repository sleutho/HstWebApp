extends layout

append head
  script.
    var study = !{JSON.stringify(study)};
  script(src="/javascripts/variables.js")
 
append backward
  p
    a(href="/studies/" + study) Back to Study

append content
  input(id="new" type="button", size="30", value="New Variable")
  table#list
    thead
      tr
        td State
        td Label
        td Varname
        td Equation
        td ParamRefAttr
        td Category
        td Role
        td Levels
        td RoleDesc
        td DataType
        td MinValueAttr
        td InitialValue
        td MaxValueAttr
        td DataMode
        td Levels
        td Distribution
        td Alpha
        td Beta
        td DataMode
        td Comment
        td
    tbody

append forward
  p
    a(href="/studies/" + study + "/responses") Next to Responses

