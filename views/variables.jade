extends layout

append head
  script.
    var study = !{JSON.stringify(study)};
  script(src="/javascripts/variables.js")
 
append backward
  p
    a(href="/studies/" + study) Back to Study

append content
  input(id="new" type="button", size="30", value="New Variable")
  table#list
    thead
      tr
        td State
        td Label
        td Varname
        td Equation
        td ParamRefAttr
        td Category
        td Role
        td Comment
    tbody

append forward
  p
    a(href="/studies/" + study + "/responses") Next to Responses

